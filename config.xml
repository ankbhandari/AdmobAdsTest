<?xml version='1.0' encoding='utf-8'?>
<widget id="com.appfeel.admob.test" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AdMobTest</name>
    <description>
    A sample Cordova and Phonegap CLI application with Admob ads integrated.
  </description>
    <author email="info@appfeel.com" href="https://github.com/appfeel/admob-google-cordova">AppFeel</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="LogLevel" value="VERBOSE" />
    <preference name="debuggable" value="true" />
    <icon src="res/appFeel-512.png" />
    <platform name="android">
        <icon density="ldpi" src="res/android/ic_launcher_ldpi.png" />
        <icon density="mdpi" src="res/android/ic_launcher_mdpi.png" />
        <icon density="hdpi" src="res/android/ic_launcher_hdpi.png" />
        <icon density="xhdpi" src="res/android/ic_launcher_xhdpi.png" />
        <icon density="xxhdpi" src="res/android/ic_launcher_xxhdpi.png" />
        <icon density="xxxhdpi" src="res/android/ic_launcher_xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <preference name="BackupWebStorage" value="local" />
        <icon height="60" src="res/ios/icon-60.png" width="60" />
        <icon height="120" src="res/ios/icon-60@2x.png" width="120" />
        <icon height="76" src="res/ios/icon-76.png" width="76" />
        <icon height="152" src="res/ios/icon-76@2x.png" width="152" />
        <icon height="40" src="res/ios/icon-40.png" width="40" />
        <icon height="80" src="res/ios/icon-40@2x.png" width="80" />
        <icon height="57" src="res/ios/icon.png" width="57" />
        <icon height="114" src="res/ios/icon@2x.png" width="114" />
        <icon height="72" src="res/ios/icon-72.png" width="72" />
        <icon height="144" src="res/ios/icon-72@2x.png" width="144" />
        <icon height="29" src="res/ios/icon-Small.png" width="29" />
        <icon height="58" src="res/ios/icon-Small@2x.png" width="58" />
        <icon height="50" src="res/ios/icon-Small-50.png" width="50" />
        <icon height="100" src="res/ios/icon-Small-50@2x.png" width="100" />
    </platform>
    <engine name="ios" spec="~4.4.0" />
    <engine name="android" spec="~6.2.3" />
    <plugin name="cordova-admob" spec="^4.1.16" />
</widget>
